def inventory_snmp_info(checktype, info):
    if len(info) >= 4:
        return [ (None, None, None) ]
    else:
        return []

def check_snmp_info(checktype, params, info):
    if len(info) >= 4:
        return (0, "OK - %s, %s, %s, %s" % (info[0][-1], info[2][-1], info[3][-1], info[1][-1]))
    else:
        return (3, "UNKNOWN - No data retrieved")

check_info['snmp_info'] = (check_snmp_info, "SNMP Info", 0, inventory_snmp_info)
snmp_info_single['snmp_info'] = ( None, ".1.3.6.1.2.1.1", [ "1.0", "4.0", "5.0", "6.0" ] )

