def inventory_cisco_fan(checkname, info):
   return [ (line[0], '', '""') for line in info ]

def check_cisco_fan(item, params, info):
   for line in info:
      if line[0] == item:
         if line[1] == "1":
            return (0, "OK")
         else:
            return (2, "CRITICAL")
   return (3, "UNKNOWN - item not found in snmp data")

check_info['cisco_fan'] = (check_cisco_fan, "%s", 0,  inventory_cisco_fan)
snmp_info['cisco_fan'] = ( "enterprises.9.9.13.1.4.1", [ 2, 3 ] ) # CISCO-SMI
