def parse_omreport(info):
    result = {}
    current_obj = {}

    def insert(obj):
        result[obj["ID"]] = obj.copy()
        obj.clear()

    for line in info:
        try:
            idx = line.index(":")
        except ValueError:
            # no colon in the line
            continue
        key = " ".join(line[:idx])
        value = " ".join(line[idx + 1:])
        if key == "ID" and current_obj:
            insert(current_obj)

        current_obj[key] = value

    insert(current_obj)
    return result


def status_translate_omreport(code):
    return {
        "ok"           : 0,
        "non-critical" : 1,
        "critical"     : 2,
        "not found"    : 3
    }.get(code.lower(), 2)

